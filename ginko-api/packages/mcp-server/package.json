{
  "name": "@ginko/mcp-server",
  "version": "1.0.0",
  "description": "MCP server for intelligent context management in Claude Code sessions",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "dashboard:dev": "cd dashboard && npm run dev",
    "dashboard:build": "cd dashboard && npm run build",
    "dashboard:start": "cd dashboard && npm run start",
    "dashboard:deploy": "cd dashboard && npm run deploy",
    "dashboard:deploy:preview": "cd dashboard && npm run deploy:preview",
    "vercel:install": "cd dashboard && npm ci",
    "vercel:build": "cd dashboard && npm run build",
    "vercel:dev": "cd dashboard && npm run dev",
    "install:all": "npm ci && cd dashboard && npm ci",
    "build:all": "npm run build && cd dashboard && npm run build"
  },
  "keywords": [
    "mcp",
    "context",
    "claude",
    "ai",
    "development"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@supabase/supabase-js": "^2.53.0",
    "@types/pg": "^8.15.5",
    "@vercel/node": "^3.0.21",
    "@ginko/shared": "*",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.6.1",
    "pg": "^8.16.3",
    "stripe": "^14.21.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "types": "dist/index.d.ts"
}
