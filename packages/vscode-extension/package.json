{
  "name": "ginko-vscode",
  "displayName": "Ginko - AI Context Management",
  "description": "Git-native session handoffs and context management for AI pair programming",
  "version": "0.1.0",
  "publisher": "ginkoai",
  "repository": {
    "type": "git",
    "url": "https://github.com/ginkoai/ginko"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "AI",
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "ai",
    "copilot",
    "cursor",
    "context",
    "handoff",
    "session"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "chatParticipants": [
      {
        "id": "ginko",
        "name": "Ginko",
        "description": "AI context management and session handoffs",
        "isSticky": true,
        "commands": [
          {
            "name": "start",
            "description": "Start a new ginko session"
          },
          {
            "name": "handoff",
            "description": "Save session progress"
          },
          {
            "name": "vibecheck",
            "description": "Quick realignment check"
          },
          {
            "name": "context",
            "description": "Load context modules"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "ginko.start",
        "title": "Ginko: Start Session",
        "category": "Ginko"
      },
      {
        "command": "ginko.handoff",
        "title": "Ginko: Create Handoff",
        "category": "Ginko"
      },
      {
        "command": "ginko.vibecheck",
        "title": "Ginko: Vibecheck",
        "category": "Ginko"
      }
    ],
    "configuration": {
      "title": "Ginko",
      "properties": {
        "ginko.enableAIEnhancement": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI enhancement for handoffs"
        },
        "ginko.autoLoadContext": {
          "type": "boolean",
          "default": true,
          "description": "Automatically load context on session start"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ginko",
          "title": "Ginko",
          "icon": "$(notebook)"
        }
      ]
    },
    "views": {
      "ginko": [
        {
          "id": "ginko.sessions",
          "name": "Sessions",
          "type": "tree"
        },
        {
          "id": "ginko.context",
          "name": "Context Modules",
          "type": "tree"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/node": "20.x",
    "typescript": "^5.5.0"
  },
  "dependencies": {}
}