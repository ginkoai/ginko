{
  "name": "@ginkoai/cli",
  "version": "2.4.5",
  "description": "Git-native session management and cloud context for AI-mediated development. AI Collaboration for Vibe Tribes.",
  "main": "dist/index.js",
  "bin": {
    "ginko": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/index.js && npm run copy:templates",
    "copy:templates": "mkdir -p dist/templates && cp src/templates/*.md dist/templates/ 2>/dev/null || true && rm -rf dist/templates/skills && cp -r src/templates/skills dist/templates/skills 2>/dev/null || true",
    "dev": "tsc --watch",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "development",
    "git",
    "privacy",
    "cli",
    "context",
    "session",
    "handoff",
    "reflection",
    "claude",
    "copilot",
    "cursor"
  ],
  "author": "Ginko AI",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ginkoai/ginko.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/ginkoai/ginko#readme",
  "bugs": {
    "url": "https://github.com/ginkoai/ginko/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@types/node": "^20.19.9",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.9",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "dotenv": "^16.6.1",
    "fs-extra": "^11.3.1",
    "glob": "^7.2.3",
    "gray-matter": "^4.0.3",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "open": "^10.2.0",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "simple-git": "^3.28.0",
    "typescript": "^5.9.2",
    "uuid": "^13.0.0",
    "yaml-front-matter": "^4.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@types/prompts": "^2.4.9",
    "@types/yaml-front-matter": "^4.1.3",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.5"
  }
}
